{
  "name": "node-boot",
  "version": "2.0.1",
  "description": "Dependency injection and RESTful api manager for node.JS via typescript",
  "main": "./lib/index.js",
  "typings": "./lib/index",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "node ./node_modules/.bin/del lib && node ./node_modules/.bin/tsc",
    "test": "node ./node_modules/mocha/bin/_mocha --compilers ts:ts-node/register --recursive src/test/**/*",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglascvas/node-boot.git"
  },
  "keywords": [
    "di",
    "typescript",
    "dependency",
    "injection",
    "ioc"
  ],
  "author": "Douglas C. Vasconcelos",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/douglascvas/node-boot/issues"
  },
  "homepage": "https://github.com/douglascvas/node-boot#readme",
  "dependencies": {
    "glob": "^7.1.1",
    "log4js": "^1.1.0",
    "reflect-metadata": "^0.1.10",
    "source-map-support": "^0.4.10"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.37",
    "@types/glob": "^5.0.32",
    "@types/minimatch": "^2.0.29",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.4",
    "@types/sinon": "^2.3.3",
    "@types/source-map-support": "^0.2.28",
    "@types/supertest": "^2.0.3",
    "chai": "^4.1.1",
    "del-cli": "^0.2.1",
    "express": "^4.15.4",
    "mocha": "^3.5.0",
    "require": "^2.4.20",
    "sinon": "^3.2.1",
    "supertest": "^3.0.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  }
}
